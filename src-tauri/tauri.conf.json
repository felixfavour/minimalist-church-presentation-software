{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run generate",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:3000",
    "distDir": "../.output/public"
  },
  "package": {
    "productName": "Cloud of Worship",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "../public/cloud-w-144.png",
        "../public/cloud-w-192.png",
        "../public/cloud-w-512.png",
        "../public/cloud-w.icns",
        "../public/favicon.ico"
      ],
      "identifier": "com.favourfelix.cloudofworshippers",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Cloud of Worship",
        "width": 800,
        "url": "/"
      },
      {
        "label": "live",
        "fullscreen": true,
        "height": 600,
        "resizable": true,
        "title": "ðŸ”´ LIVE - Cloud of Worship",
        "width": 800,
        "url": "/live"
      }
    ]
  }
}
